{
  "name": "beegood",
  "version": "1.0.0",
  "description": "A good worker bee to keep do your repo work for you",
  "author": "Gonzalo Peci <pecigonzalo@users.noreply.github.com> (https://github.com/pecigonzalo/beegood)",
  "license": "ISC",
  "repository": "https://github.com/pecigonzalo/beegood.git",
  "homepage": "https://github.com/pecigonzalo/beegood",
  "bugs": "https://github.com/pecigonzalo/beegood/issues",
  "keywords": [
    "probot",
    "github",
    "probot-app"
  ],
  "scripts": {
    "dev": "nodemon",
    "start": "probot run ./index.js",
    "lint": "standard --fix",
    "test": "jest && standard",
    "test:watch": "jest --watch --notify --notifyMode=change --coverage"
  },
  "dependencies": {
    "probot": "^7.2.0",
    "probot-scheduler": "^2.0.0-beta.1"
  },
  "devDependencies": {
    "jest": "^24.0.0",
    "nock": "^10.0.1",
    "nodemon": "^1.17.2",
    "smee-client": "^1.0.2",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">= 8.3.0"
  },
  "standard": {
    "env": [
      "jest"
    ]
  },
  "nodemonConfig": {
    "exec": "npm start",
    "watch": [
      ".env",
      "."
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
